  "description": "Portal for end users where they can easily request IT assistance",
  "productName": "HC IT Portal",
  "author": "DarkLite1 <BrechtGijbels@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "env-cmd -e dev quasar dev",
    "quasar-clean": "quasar clean",
    "quasar-build": "quasar build",
    "quasar-serve": "quasar serve dist/spa --open",
    "build": "run-s quasar-clean quasar-build",
    "build-and-serve": "run-s quasar-clean quasar-build quasar-serve",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore ./",
    "test": "echo \"No test specified\" && exit 0",
    "codegen": "env-cmd -e dev graphql-codegen",
    "codegen-watch": "env-cmd -e dev graphql-codegen --watch"
  },
