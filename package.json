    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "env-cmd": "^10.1.0",
    "global": "^4.4.0",
    "graphql": "^15.3.0",
